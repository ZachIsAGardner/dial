{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "dial",
	"patterns": [
		{
			"include": "#keywords"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword",
					"match": "^\\s*\\[.*$"
				},
				{
					"name": "keyword",
					"match": "<(.*?)>"
				},
				{
					"name": "comment",
					"match": "^\\s*#.*$"
				},
				{
					"name": "keyword.control",
					"match": "^\\s*@.*$"
				},
				{
					"name": "entity.name.type",
					"begin": "{",
					"end": "}"
				},
				{
					"name": "keyword.control",
					"match": "\\?Q"
				},
				{
					"name": "keyword.control",
					"match": "\\?B"
				},
				{
					"name": "keyword.control",
					"match": "=>"
				},
				{
					"name": "entity.name.type",
					"match": "(?<=\\=>.*).*"
				},
				{
					"name": "entity.name.type",
					"match": "((.*)=)(?![^<]*>)"
				},
				{
					"name": "keyword",
					"match": "(\\(.*\\)).*(?==>)"
				},
				{
					"name": "keyword",
					"match": "\\(([^\\)]+)\\).*(?==)"
				},
				{
					"name": "invalid",
					"match": "!TODO!"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.dial",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.dial",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "text.dial"
}